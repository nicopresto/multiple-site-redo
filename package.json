{
  "name": "lerna-monorepo-starter",
  "description": "Lerna monorepo starter",
  "version": "1.0.0",
  "author": "Andrico Karoulla <karoulla.andrico@gmail.com>",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "lerna": "^2.11.0",
    "react": "16.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest test --config ./json-config.json",
    "run:blog": "cd packages/blog && yarn develop",
    "run:shop": "cd packages/shop && yarn develop",
    "run:all": "npm-run-all --parallel run:blog run:shop"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "babel-jest": "^23.4.2",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "react-router-dom": "^4.3.1",
    "babel-plugin-transform-export-extensions": "*",
    "jest-cli": "*",
    "react-test-renderer": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
